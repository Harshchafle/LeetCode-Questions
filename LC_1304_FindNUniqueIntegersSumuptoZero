class Solution {
    public int[] sumZero(int n) {
        int[] ans = new int[n];
        int st = 0;
        if((n&1) != 0) ans[st++] = 0;
        int nums = 1;
        while(st < n-1){
            ans[st++] = -1*nums;
            ans[st++] = nums;
            nums++;
        }
        return ans;
    }
}
