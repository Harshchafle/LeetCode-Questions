class Solution {
    public int numOfUnplacedFruits(int[] fruits, int[] baskets) {
        int n = fruits.length;
        int placed = 0;
        int done[] = new int[n];

        for(int i = 0; i < n; i++){
            for(int j = 0; j < n; j++){
                if(done[j] == 0 && baskets[j] >= fruits[i]){
                    done[j] = fruits[i];
                    placed++;
                    break;
                }
            }
        }
        return n-placed;
    }
}
